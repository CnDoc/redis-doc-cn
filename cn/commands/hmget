    <div class='text'>
      <h1 class='command'>
        <span class='name'>HMGET</span>
        <span class='arg'>key</span>
        <span class='arg'>field [field ...]</span>
      </h1>
      <article>
        <aside>
          <h2>
            Related commands
          </h2>
